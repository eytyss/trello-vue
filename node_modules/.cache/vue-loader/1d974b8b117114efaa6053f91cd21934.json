(list){\n          return this.cards.filter( item => item.listId == list.id)\n      },\n      addCard(name) {\n        if (name.length > 0) {\n          this.$store.state.lastCardId++;\n          let card = {\n            listId: this.list.id,\n            id: this.$store.state.lastCardId,\n            name: name\n          };\n\n          this.cards.push(card);\n          this.listName = ''\n        }\n        },\n        toggle(data){\n          const currentData = {\n              listId: this.list.id,\n              listName: this.list.name,\n              id: data.id,\n              name: data.name,\n            };\n          this.$store.commit(\"toggleOverlay\")\n          this.$store.commit(\"openForm\", currentData);\n      }\n\n    }\n}\n</script>\n\n<style>\n.input-card {\n  position: relative;\n  background-color: white;\n  min-height: 30px;\n  width: 80%;\n  display: flex;\n  align-items: center;\n  border-radius: 5px;\n  padding: 5px;\n  word-break: break-all;\n  font-size: 16px;\n  outline: none;\n  border:none\n\n}\n.btn-delete{\n  background: inherit;\n  border:none;\n  font-weight:bold\n}\n\n</style>"]}]}